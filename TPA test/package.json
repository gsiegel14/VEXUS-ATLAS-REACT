{
  "name": "dhrem-research",
  "version": "0.0.0",
  "description": "Denver Health Research & Emergency Medicine Faculty Page",
  "private": true,
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "node tpaserver.js",
    "backend": "node tpaserver.js",
    "dev:all": "concurrently \"npm run backend\" \"npm run dev\"",
    "dev:full": "concurrently \"npm run backend\" \"npm run dev\"",
    "deploy": "./deploy.sh",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
