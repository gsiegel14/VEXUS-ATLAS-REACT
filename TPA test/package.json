    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@types/cropperjs": "^1.1.5",
    "airtable": "^0.12.2",
    "axios": "^1.9.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "cropperjs": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "express": "^4.19.2",
