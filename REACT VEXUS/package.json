{
  "name": "vexus-atlas-react",
  "version": "1.0.0",
  "description": "VEXUS ATLAS React app",
  "private": true,
  "scripts": {
    "dev": "vite --port 5436",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server.js",
    "deploy:setup": "chmod +x setup-production.sh && ./setup-production.sh",
    "deploy:build": "gcloud builds submit --tag gcr.io/decoded-app-457000-s2/vexus-atlas",
    "deploy:run": "gcloud run deploy vexus-atlas --image gcr.io/decoded-app-457000-s2/vexus-atlas --platform managed --region us-central1",
    "deploy": "npm run deploy:build && npm run deploy:run",
    "test:endpoints": "node test-endpoints.js"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@google-cloud/secret-manager": "^5.6.0",
    "@mui/icons-material": "^5.14.19",
    "@mui/material": "^5.14.20",
    "@types/cropperjs": "^1.1.5",
    "airtable": "^0.12.2",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "cropperjs": "^2.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "react": "^18.2.0",
    "react-cropper": "^2.3.3",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.56.4",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.20.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.1.1",
    "concurrently": "^9.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-darwin-arm64": "^4.41.1"
  }
}